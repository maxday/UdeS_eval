<h1>Liste des sessions</h1>

<table>
  <tr>
    <th>Nom</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quarters.each do |quarter| %>
  <tr>
    <td><%= quarter.name %></td>
    <td><%= link_to 'Voir', quarter %></td>
    <td><%= link_to 'Editer', edit_quarter_path(quarter) %></td>
    <td><%= link_to 'Supprimer', quarter, confirm: 'Êtes-vous sûr ?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

